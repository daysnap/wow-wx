  "description": "webpack building multiple page projects",
  "main": "index.js",
  "scripts": {
    "dev": "node cmd -d dist -r app=wow::env=dev -p home_index&&webpack --watch --config cmd/webpack.config.js",
    "test": "node cmd -d dist -r app=wow::env=test -p home_index&&webpack --watch --config ./cmd/webpack.config.js",
    "build": "node cmd -d dist -r app=wow::env=build -p home_index&&webpack --watch --config ./cmd/webpack.config.js",
    "clear": "node cmd -d dist",
    "router": "node cmd -p home_index",
    "serve": "serve -p 10011"
  },
